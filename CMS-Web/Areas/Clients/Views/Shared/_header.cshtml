@{
    var Cate = Session["Catelogies"] as CMS_DTO.CMSSession.CateSession;
}

<div class="header-area ptb-30 header-sticky">
    <div class="container">
        <div class="row">
            <div class="col-md-3 col-xs-12">
                <div class="logo">
                    <a href="@Url.Action("Index","Home")"><img src="@Url.Content("~/Images/logo.png")" alt="KOFFEE" /></a>
                </div>
            </div>
            <div class="col-md-9 col-xs-12">
                <div class="content-wrapper">
                    <!-- Main Menu Start -->
                    <div class="main-menu text-right">
                        <nav>
                            <ul>
                                <li><a href="@Url.Action("Index","Home")">Trang chủ</a></li>
                                <li><a href="@Url.Action("Index","About")">Giới thiệu</a></li>
                                <li><a href="@Url.Action("Index","Product")">Sản phẩm</a></li>
                                <li><a href="@Url.Action("Index","Process")">Tiến trình</a></li>
                                <li><a href="@Url.Action("Index","News")">Tin tức</a></li>
                                <li><a href="@Url.Action("Index","Contact")">Liên hệ</a></li>
                            </ul>
                        </nav>
                    </div>
                    <div class="mobile-menu hidden-lg hidden-md"></div>
                    <!-- Main Menu End -->
                    <!--Search Form Start-->
                    <div class="search-btn">
                        <ul data-toggle="dropdown" class="header-search search-toggle">
                            <li class="search-menu">
                                <i class="zmdi zmdi-search"></i>
                            </li>
                        </ul>
                        <div class="search">
                            <div class="search-form">
                                <form id="search-form" action="/Clients/Home/Search" method="get">
                                    <input type="search" placeholder="Tìm kiếm..." name="search" />
                                    <button type="submit">
                                        <span><i class="zmdi zmdi-search"></i></span>
                                    </button>
                                </form>
                            </div>
                        </div>
                    </div>
                    <!--End of Search Form-->
                </div>
            </div>
        </div>
    </div>
</div>

